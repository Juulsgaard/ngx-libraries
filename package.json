{
  "name": "ngx-libraries",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npx syncpack fix-mismatches && npx syncpack set-semver-ranges",
    "version:start": "npx changeset",
    "version:apply": "npx changeset version",
    "version:publish": "npm run build:all && npx changeset publish",
    "build:all": "ng build ngx-tools && ng build ngx-material && ng build ngx-service-worker && ng build ngx-forms && ng build ngx-forms-inputs",
    "link:ngx-forms": "npm run build -w @consensus-labs/ngx-forms && cd dist/ngx-forms && yalc push"
  },
  "workspaces": [
    "./projects/*"
  ],
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.0",
    "@angular/cli": "14.1.0",
    "@angular/compiler": "14.1.0",
    "@angular/compiler-cli": "14.1.0",
    "@changesets/cli": "^2.24.2",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.1.0",
    "rxjs": "7.5.6",
    "syncpack": "^8.2.4",
    "tslib": "^2.4.0",
    "typescript": "~4.7.2"
  },
  "dependencies": {
    "@angular/core": "14.1.0",
    "@angular/common": "14.1.0",
    "@angular/material": "14.1.0",
    "@angular/animations": "14.1.0",
    "@angular/forms": "14.1.0",
    "@consensus-labs/ngx-forms-core": "1.0.4",
    "@consensus-labs/rxjs-tools": "1.0.2",
    "@consensus-labs/ts-tools": "1.1.0"
  }
}
